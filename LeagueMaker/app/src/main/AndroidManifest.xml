<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.keith.leaguemaker" >

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LeaguesActivity" />
        <activity android:name=".ResultsActivity" />
        <activity android:name=".TeamsActivity" />
        <activity android:name=".AddLeagueActivity" />
        <activity android:name=".ModifyLeagueActivity" />
        <activity android:name=".ViewLeaguesActivity" />
        <activity android:name=".ViewTeamsActivity" />
        <activity android:name=".AddTeamActivity" />
        <activity android:name=".ModifyTeamActivity" />
        <activity android:name=".ViewResultsActivity" />
        <activity android:name=".AddResultActivity" />
        <activity android:name=".ModifyResultActivity" >
        </activity>
    </application>

</manifest>
